<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>WpNonces</title>
    <parser>
        <target>docs/api</target>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>docs/api</target>
    </transformer>
    <transformations>
        <template name="clean"/>
    </transformations>
    <files>
        <directory>src</directory>
        <ignore>tests/*</ignore>
        <ignore>Tests/*</ignore>
        <ignore>vendor/*</ignore>
    </files>
</phpdoc>
